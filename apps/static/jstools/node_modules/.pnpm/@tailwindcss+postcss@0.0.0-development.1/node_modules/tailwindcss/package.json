{
  "name": "tailwindcss",
  "version": "0.0.0-development.1",
  "scripts": {
    "build": "tsup-node --env.NODE_ENV production",
    "dev": "tsup-node --env.NODE_ENV development --watch"
  },
  "bin": {
    "tailwindcss": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib.d.mts",
      "require": "./dist/lib.js",
      "import": "./dist/lib.mjs"
    }
  },
  "dependencies": {
    "lightningcss": "^1.23.0",
    "@tailwindcss/oxide": "0.0.0-development.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.8"
  }
}
