#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/tailwindcss@0.0.0-development.1/node_modules/tailwindcss/dist/node_modules:/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/tailwindcss@0.0.0-development.1/node_modules/tailwindcss/node_modules:/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/tailwindcss@0.0.0-development.1/node_modules:/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/tailwindcss@0.0.0-development.1/node_modules/tailwindcss/dist/node_modules:/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/tailwindcss@0.0.0-development.1/node_modules/tailwindcss/node_modules:/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/tailwindcss@0.0.0-development.1/node_modules:/mnt/c/Users/THINKPAD T14/Desktop/RTZproject/RTZ/apps/static/jstools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tailwindcss@0.0.0-development.1/node_modules/tailwindcss/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../tailwindcss@0.0.0-development.1/node_modules/tailwindcss/dist/cli.js" "$@"
fi
